{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"experimentalDecorators": true,
		"downlevelIteration": true,
		"outDir": "../out-tsc/app",
		"types": [],
		"paths": {
			"@ngrx/*": ["../node_modules/@ngrx/*"],
			"@app/*": ["src/app/*"],
			"@lore/*": ["src/app/lore/*"],
			"@env/*": ["src/environments/*"]
		}
	},
	"exclude": ["test.ts", "**/*.spec.ts"]
}
