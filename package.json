	"scripts": {
		"ng": "ng",
		"start": "ng serve --aot --sourceMap=true",
		"build": "ng build --prod --aot --stats-json --base-href \"/loreplotter/\"",
		"start:prod": "http-server ./dist/ -c-1 --gzip -o http://localhost:8080/loreplotter",
		"test": "ng test",
		"test:headless": "ng test --watch=false --browsers=ChromeHeadless",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"format": "npx prettier --write \"src/**/*\"",
		"build:stats": "npm run build --stats-json",
		"start:stats": "webpack-bundle-analyzer dist/loreplotter/stats.json"
	},
	"private": true,
