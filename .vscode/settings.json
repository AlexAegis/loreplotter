{
	"tslint.alwaysShowRuleFailuresAsWarnings": true,
	"editor.formatOnSave": true,
	"editor.autoIndent": true,
	"editor.tabCompletion": "off",
	"editor.quickSuggestions": {
		"other": true,
		"comments": false,
		"strings": false
	},
	"editor.wordBasedSuggestions": true,
	"editor.suggest.localityBonus": true,
	"editor.snippetSuggestions": "bottom",
	"editor.quickSuggestionsDelay": 0,
	"editor.tabSize": 4,
	"editor.codeActionsOnSave": {
		"source.fixAll": true
	},
	"prettier.useTabs": true,
	"prettier.tabWidth": 4,
	"prettier.singleQuote": true,
	"prettier.eslintIntegration": true,
	"material-icon-theme.folders.associations": {
		"module": "font",
		"component": "components",
		"doc": "markdown",
		"animation": "delta",
		"guard": "ngrx-effects",
		"directive": "ngrx-actions",
		"interceptor": "middleware",
		"validator": "coverage",
		"e2e": "test",
		"store": "Redux-store",
		"actions": "ngrx-actions",
		"action": "ngrx-actions",
		"effects": "Ngrx-effects",
		"effect": "Ngrx-effects",
		"reducers": "ngrx-reducer",
		"reducer": "ngrx-reducer"
	},
	"material-icon-theme.files.associations": {
		"*.animation.ts": "purescript"
	},
	"files.exclude": {
		"**/.git": true,
		"**/.DS_Store": true,
		"node_modules": true,
		"coverage": true
	},
	"travis.repository": "loreplotter",
	"docthis.authorName": "AlexAegis",
	"docthis.includeTypes": false,
	"docthis.includeMemberOfOnClassMembers": false,
	"search.usePCRE2": true,
	"terminal.integrated.windowsEnableConpty": true,
	"plantuml.diagramsRoot": "docs/",
	"plantuml.exportOutDir": "dist/",
	"latex-workshop.latex.magic.args": ["-shell-escape", "%DOC%"],
	"typescript.tsdk": "node_modules\\typescript\\lib",
	"editor.fontFamily": "'Fira Code Regular', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
	"editor.mouseWheelZoom": true,
	"editor.wordWrap": "bounded",
	"latex-workshop.latex.recipes": [
		{
			"name": "lualatex ðŸ”ƒ",
			"tools": ["lualatex"]
		},
		{
			"name": "lualatex âžž bibtex âžž lualatex`Ã—2",
			"tools": ["lualatex", "bibtex", "lualatex", "lualatex"]
		},
		{
			"name": "latexmk ðŸ”ƒ",
			"tools": ["latexmk"]
		},
		{
			"name": "pdflatex âžž bibtex âžž pdflatex`Ã—2",
			"tools": ["pdflatex", "bibtex", "pdflatex", "pdflatex"]
		}
	],
	"latex-workshop.latex.tools": [
		{
			"name": "lualatex",
			"command": "lualatex",
			"args": ["-synctex=1", "-file-line-error", "--shell-escape", "-interaction=nonstopmode", "%DOC%"],
			"env": {}
		},
		{
			"name": "latexmk",
			"command": "latexmk",
			"args": [
				"-synctex=1",
				"-interaction=nonstopmode",
				"--shell-escape",
				"-file-line-error",
				"-pdf",
				"-outdir=%OUTDIR%",
				"%DOC%"
			],
			"env": {}
		},
		{
			"name": "pdflatex",
			"command": "pdflatex",
			"args": ["-synctex=1", "-interaction=nonstopmode", "--shell-escape", "-file-line-error", "%DOC%"],
			"env": {}
		},
		{
			"name": "bibtex",
			"command": "bibtex",
			"args": ["%DOCFILE%"],
			"env": {}
		}
	],
	"workbench.colorTheme": "Material Theme Darker High Contrast"
}
